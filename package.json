        "axios": "^0.21.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router-dom": "5.0.1",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "semantic-ui-react": "0.87.2",
        "ts-loader": "^6.0.4",
        "typescript": "^3.5.2",
        "typestyle": "^2.0.4"
